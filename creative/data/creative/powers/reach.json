{
    "name": "Long Arms",
    "description": "you can reach very far",
    "interval": 20,
    "type": "origins:action_over_time",
    "rising_action": {
        "type": "origins:execute_command",
        "command": "scale set pehkui:reach 5"
    },
    "falling_action": {
        "type": "origins:execute_command",
        "command": "scale reset pehkui:reach"
    },  
    "condition": {
        "type": "origins:gamemode",
        "gamemode": "creative"
    }
}